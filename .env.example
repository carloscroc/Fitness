# API Keys
GEMINI_API_KEY=PLACEHOLDER_API_KEY

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Feature Flags - Exercise Migration
# Enable backend exercises loading (true/false)
VITE_FF_ENABLE_BACKEND_EXERCISES=false

# Show data source indicators in development (true/false)
VITE_FF_SHOW_DATA_SOURCE_INDICATORS=false

# Auto-create GitHub issues for missing data (true/false)
VITE_FF_AUTO_CREATE_GITHUB_ISSUES=true

# Enable percentage-based rollout (true/false)
VITE_FF_ENABLE_PERCENTAGE_ROLLOUT=false

# Debug mode for exercise merging logic (true/false)
VITE_FF_DEBUG_EXERCISE_MERGING=false

# Rollout Configuration
# Percentage of users who should see backend exercises (0-100)
VITE_ROLLOUT_PERCENTAGE=0

# GitHub Configuration (for automatic issue creation)
# Only needed if AUTO_CREATE_GITHUB_ISSUES is enabled
GITHUB_TOKEN=your_github_token_here
GITHUB_REPO=your_github_repo_here
GITHUB_OWNER=your_github_owner_here

# Development Settings
# Enable development-only features
NODE_ENV=development

# Cache Configuration
# Exercise cache TTL in milliseconds (default: 24 hours)
EXERCISE_CACHE_TTL=86400000

# Performance Settings
# Enable performance monitoring
ENABLE_PERFORMANCE_MONITORING=false